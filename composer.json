{
    "name": "learnerbot/php-in-docker",
    "description": "PHP in Docker example.",
    "keywords": ["php", "docker", "example"],
    "license": "MIT",
    "type": "project",
    "require":
    {
        "php": ">=5.6.4"
    },
    "require-dev":
    {},
    "autoload":
    {
        "classmap": [],
        "psr-4":
        {
            "App\\": "app/"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Tests\\": "tests/"
        }
    },
    "scripts":
    {},
    "config":
    {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}